. $HOME/dotfiles-local/zsh/z/z.sh

# Brew path
export PATH=/usr/local/bin:${PATH}

# GPG Key TTY
export GPG_TTY=$(tty)

function todo {
  touch ~/.todo
  local todo
  todo="$*"
  echo $todo >> ~/.todo
  cat -n ~/.todo
}

# GOPATH
if command -v go > /dev/null
then
  mkdir -p $HOME/workspace/go
  export GOPATH=$HOME/workspace/go
fi

# RBenv init
eval "$(rbenv init -)"
